<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment>SWModule encrypt with JKS.</comment>
    <entry key="check.certificate.validity.period">false</entry>

    <!-- Key store configuration -->
    <entry key="keyStorePath">keystores/xa-ec.jks</entry>
    <entry key="keyStorePassword">Test1234</entry>
    <entry key="keyPassword">Test1234</entry>
    <entry key="keyStoreType">JKS</entry>

    <!-- Management of the encryption activation -->
    <entry key="encryptionActivation">encryptionConf.xml</entry>

    <!-- Decryption config -->
    <entry key="responseDecryptionIssuer">CN=Test CEF eIDAS node XA, OU=eIDAS node org unit, O=eIDAS node org, C=XA</entry>
    <entry key="serialNumber">62B37992</entry>

</properties>