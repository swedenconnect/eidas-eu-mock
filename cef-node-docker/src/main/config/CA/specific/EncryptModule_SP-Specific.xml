<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment>SWModule encrypt with JKS.</comment>
    <entry key="check_certificate_validity_period">false</entry>
    <entry key="disallow_self_signed_certificate">false</entry>


    <entry key="response.encryption.mandatory">true</entry>
    <!-- Data Encryption algorithm -->
    <entry key="data.encryption.algorithm">http://www.w3.org/2009/xmlenc11#aes256-gcm</entry>
    <!-- Decryption algorithm Whitelist-->
    <entry key="encryption.algorithm.whitelist">
        http://www.w3.org/2009/xmlenc11#aes128-gcm;
        http://www.w3.org/2009/xmlenc11#aes256-gcm;
        http://www.w3.org/2009/xmlenc11#aes192-gcm
    </entry>
    <!-- Key Encryption algorithm -->
    <entry key="key.encryption.algorithm">http://www.w3.org/2001/04/xmlenc#rsa-oaep-mgf1p</entry>
    <entry key="keyStorePath">../../keystore/eidasKeyStore_Connector_CA.jks</entry>
    <entry key="keyStorePassword">local-demo</entry>
    <entry key="keyPassword">local-demo</entry>
    <!-- For C-PEPS[1..*] - S-PEPS[1] communication additional country code feature must be added to key management! -->
    <!-- Management of the encryption activation -->
    <entry key="encryptionActivation">../encryptionConf.xml</entry>

    <!--  If not present then no encryption will be applied on response -->
    <entry key="responseToPointIssuer.CA">CN=sp-ca-demo-certificate, OU=STORK, O=SP, L=EU, ST=EU, C=CA</entry>
    <entry key="responseToPointSerialNumber.CA">54C8FB33</entry>

    <entry key="responseToPointIssuer.CB">CN=sp-cb-demo-certificate, OU=STORK, O=SP, L=EU, ST=EU, C=CB</entry>
    <entry key="responseToPointSerialNumber.CB">54C8FB8F</entry>

    <entry key="responseToPointIssuer.CC">CN=sp-cc-demo-certificate, OU=STORK, O=SP, L=EU, ST=EU, C=CC</entry>
    <entry key="responseToPointSerialNumber.CC">54C8FBC5</entry>

    <entry key="responseToPointIssuer.CD">CN=sp-cd-demo-certificate, OU=STORK, O=SP, L=EU, ST=EU, C=CD</entry>
    <entry key="responseToPointSerialNumber.CD">54C8FC0A</entry>

    <entry key="responseToPointIssuer.CF">CN=sp-cf-demo-certificate, OU=STORK, O=SP, L=EU, ST=EU, C=CF</entry>
    <entry key="responseToPointSerialNumber.CF">54D78904</entry>

    <!--  If not present then no decryption will be applied on response -->
    <entry key="responseDecryptionIssuer">CN=speps-ca-demo-certificate, OU=STORK, O=SPEPS, L=EU, ST=EU, C=CA</entry>
    <entry key="serialNumber">54C8FC88</entry>

    <entry key="keyStoreType">JKS</entry>
</properties>