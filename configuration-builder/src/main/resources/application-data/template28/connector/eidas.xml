<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <!-- Connector  -->
    <!-- Connector assertion URL -->
  <entry key="connector.assertion.url">${config.base-url}/EidasNodeConnector/ColleagueResponse</entry>

    <!--connector Metadata -->
  <entry key="connector.metadata.url">${config.metadata-url}/EidasNodeConnector/ConnectorMetadata</entry>

    <!-- METADATA configuration -->
    <!-- Connector contact -->
  <entry key="connector.contact.support.email">${metadata.contact.support.email}</entry>
  <entry key="connector.contact.support.company">${metadata.contact.support.company}</entry>
  <entry key="connector.contact.support.givenname">${metadata.contact.support.givenname}</entry>
  <entry key="connector.contact.support.surname">${metadata.contact.support.surname}</entry>
  <entry key="connector.contact.support.phone">${metadata.contact.support.phone}</entry>
  <entry key="connector.contact.technical.email">${metadata.contact.technical.email}</entry>
  <entry key="connector.contact.technical.company">${metadata.contact.technical.company}</entry>
  <entry key="connector.contact.technical.givenname">${metadata.contact.technical.givenname}</entry>
  <entry key="connector.contact.technical.surname">${metadata.contact.technical.surname}</entry>
  <entry key="connector.contact.technical.phone">${metadata.contact.technical.phone}</entry>
    <!-- Connector organization -->
  <entry key="connector.organization.name">${metadata.organization.name}</entry>
  <entry key="connector.organization.displayname">${metadata.organization.displayname}</entry>
  <entry key="connector.organization.url">${metadata.organization.url}</entry>

${metadata.sector}

    <!-- Number of known Proxy-Service -->
  <entry key="service.number">${config.service-count}</entry>

${service.information}

    <!-- SECURITY POLICY -->
    <!-- uri used for the CSP reporting feature-->
    <entry key="security.header.CSP.report.uri">${config.base-url}/EidasNodeConnector</entry>

    <!-- value of the NodeCountry to be published in the metadata-->
    <entry key="metadata.node.country">${config.country}</entry>

	<!--Restrictmetadataretrievaltohttps-->
	<entry key="metadata.restrict.http">false</entry>
    <!--URL for specific-connector response receiver -->
    <entry key="specific.connector.response.receiver">${config.base-url}/SpecificConnector/ConnectorResponse</entry>

</properties>
