<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:samlec="urn:ietf:params:xml:ns:samlec" xmlns="http://www.opensaml.org/xmltooling-config" xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../src/schema/xmltooling-config.xsd">
 
    <!-- SAML-EC GSS-API Object providers -->
    <ObjectProviders>
        
        <!-- EncType -->
        <ObjectProvider qualifiedName="samlec:EncType">
            <BuilderClass className="org.opensaml.samlext.samlec.impl.EncTypeBuilder"/>
			<MarshallingClass className="org.opensaml.xml.schema.impl.XSStringMarshaller"/>
			<UnmarshallingClass className="org.opensaml.xml.schema.impl.XSStringUnmarshaller"/>
        </ObjectProvider>

        <!-- GeneratedKey -->
        <ObjectProvider qualifiedName="samlec:GeneratedKey">
            <BuilderClass className="org.opensaml.samlext.samlec.impl.GeneratedKeyBuilder"/>
            <MarshallingClass className="org.opensaml.samlext.samlec.impl.GeneratedKeyMarshaller"/>
            <UnmarshallingClass className="org.opensaml.samlext.samlec.impl.GeneratedKeyUnmarshaller"/>
        </ObjectProvider>

        <ObjectProvider qualifiedName="samlec:GeneratedKeyType">
            <BuilderClass className="org.opensaml.samlext.samlec.impl.GeneratedKeyBuilder"/>
            <MarshallingClass className="org.opensaml.samlext.samlec.impl.GeneratedKeyMarshaller"/>
            <UnmarshallingClass className="org.opensaml.samlext.samlec.impl.GeneratedKeyUnmarshaller"/>
        </ObjectProvider>

        <!-- SessionKey -->
        <ObjectProvider qualifiedName="samlec:SessionKey">
            <BuilderClass className="org.opensaml.samlext.samlec.impl.SessionKeyBuilder"/>
            <MarshallingClass className="org.opensaml.samlext.samlec.impl.SessionKeyMarshaller"/>
            <UnmarshallingClass className="org.opensaml.samlext.samlec.impl.SessionKeyUnmarshaller"/>
        </ObjectProvider>

        <ObjectProvider qualifiedName="samlec:SessionKeyType">
            <BuilderClass className="org.opensaml.samlext.samlec.impl.SessionKeyBuilder"/>
            <MarshallingClass className="org.opensaml.samlext.samlec.impl.SessionKeyMarshaller"/>
            <UnmarshallingClass className="org.opensaml.samlext.samlec.impl.SessionKeyUnmarshaller"/>
        </ObjectProvider>

    </ObjectProviders>
 </XMLTooling>
