            <UnmarshallingClass className="org.opensaml.saml1.core.impl.AttributeQueryUnmarshaller"/>
        </ObjectProvider>
        
        <ObjectProvider qualifiedName="saml1p:AttributeQueryType">
            <BuilderClass className="org.opensaml.saml1.core.impl.AttributeQueryBuilder"/>
            <MarshallingClass className="org.opensaml.saml1.core.impl.AttributeQueryMarshaller"/>
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.AttributeQueryUnmarshaller"/>
        </ObjectProvider>

        <!-- AuthenticationQuery -->
        <ObjectProvider qualifiedName="saml1p:AuthenticationQuery">
            <BuilderClass className="org.opensaml.saml1.core.impl.AuthenticationQueryBuilder"/>
