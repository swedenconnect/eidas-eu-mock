            <UnmarshallingClass className="org.opensaml.xml.schema.impl.XSQNameUnmarshaller"/>
        </ObjectProvider>

        <!-- Response -->
        <ObjectProvider qualifiedName="saml1p:Response">
            <BuilderClass className="org.opensaml.saml1.core.impl.ResponseBuilder"/>
