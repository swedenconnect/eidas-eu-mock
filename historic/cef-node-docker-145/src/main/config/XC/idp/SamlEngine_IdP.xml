	<entry key="residencePermit">http://www.stork.gov.eu/1.0/residencePermit</entry>
	<entry key="eMail">http://www.stork.gov.eu/1.0/eMail</entry>
	<entry key="academicTitle">http://www.stork.gov.eu/1.0/academicTitle</entry>
